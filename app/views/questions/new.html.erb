<h1>New question</h1>

<form action="/tests/<%= params[:test_id] %>/questions" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>
    <input type="text" name="question[body]" id="question" placeholder="Question body">
  </p>
  <br>
  <button type="submit">Create question</button>
</form>